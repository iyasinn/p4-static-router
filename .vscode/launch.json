{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Debug StaticRouter",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/StaticRouter",
			"args": ["-r", "./rtable"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build"
		},
		{
			"name": "unit test a.out",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/unit_test/a.out",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug",
			"program": "${workspaceFolder}/build/StaticRouter",
			"args": ["-r", "./rtable"],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "build"
		}
	]
}
